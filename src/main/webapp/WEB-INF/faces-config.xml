<?xml version="1.0" encoding="UTF-8"?>
<!-- This file is not required if you don't need any extra configuration. -->
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="
        http://java.sun.com/xml/ns/javaee
        http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

   <!-- This descriptor activates the JSF 2.0 Servlet -->

   <!-- Write your navigation rules here. You are encouraged to use CDI for 
      creating @Named managed beans. -->
	<application>
	 	<el-resolver>
	 		org.primefaces.application.exceptionhandler.PrimeExceptionHandlerELResolver
	 	</el-resolver>
	</application>
	<factory>
		<exception-handler-factory>
			org.primefaces.application.exceptionhandler.PrimeExceptionHandlerFactory
		</exception-handler-factory>
	</factory>
	<!-- converter>
        <converter-id>theme</converter-id>
        <converter-class>org.primefaces.showcase.convert.ThemeConverter</converter-class>
    </converter-->
	<navigation-rule>
	    <from-view-id>*</from-view-id> 
	    <navigation-case>
	        <from-outcome>introduccion</from-outcome>
            <to-view-id>/ui/introduccion.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
	        <from-outcome>cambiarContrasena</from-outcome>
            <to-view-id>/ui/cambiarContrasena.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
	        <from-outcome>salir</from-outcome>
            <to-view-id>/ui/login.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
	        <from-outcome>usuario</from-outcome>
            <to-view-id>/ui/administracion/usuarios.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
	        <from-outcome>listado_maestro_Ubicaciones</from-outcome>
            <to-view-id>/ui/maestros/ubicacion.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
	        <from-outcome>listado_maestro_Agentes</from-outcome>
            <to-view-id>/ui/maestros/agenteAduana.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
	        <from-outcome>agenciaCarga</from-outcome>
            <to-view-id>/ui/maestros/agenciaCarga.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
	        <from-outcome>lugar_incoterm</from-outcome>
            <to-view-id>/ui/maestros/lugarIncoterm.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
	        <from-outcome>consultar_ultima_asignacionLote</from-outcome>
            <to-view-id>/ui/comercioexterior/reiniciarConsecutivoOic.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
	        <from-outcome>ingresar_costos_incoterm</from-outcome>
            <to-view-id>/ui/comercioexterior/ingresarCostosIncoterm.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
	        <from-outcome>listado_FX_AsignarContribucionCafe</from-outcome>
            <to-view-id>/ui/comercioexterior/asignarDatosContribucionCafetera.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
	        <from-outcome>listado_Formato_Novedad</from-outcome>
            <to-view-id>/ui/reportes/formatoNovedades.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
	        <from-outcome>listado_maestro_ProductosPorCliente_CE</from-outcome>
            <to-view-id>/ui/maestros/productosClienteComExt.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
	        <from-outcome>incoterm_medio_transporte</from-outcome>
            <to-view-id>/ui/maestros/terminoIncotermXMedioTrans.xhtml</to-view-id>
		<navigation-case>
	        <from-outcome>maestro_inventario</from-outcome>
            <to-view-id>/ui/maestros/productos.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>
